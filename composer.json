{
    "name": "rubix/engine",
    "description": "A library for querying and performing computations on graphs.",
    "license": "MIT",
    "keywords": ["php", "graph"],
    "authors": [
        {
            "name": "Andrew DalPino",
            "email": "me@andrewdalpino.com"
        }
    ],
    "require": {
        "php": ">=7.1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "6.0.*"
    },
    "autoload": {
        "psr-4": {
            "Rubix\\Engine\\": "src/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
