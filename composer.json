{
    "name": "rubix/graph",
    "description": "A library for querying and performing computations on graphs.",
    "license": "MIT",
    "keywords": ["php", "graph", "tree", "grid", "dag", "bst", "trie"],
    "authors": [
        {
            "name": "Andrew DalPino",
            "email": "me@andrewdalpino.com"
        }
    ],
    "require": {
        "php": ">=7.1.3"
    },
    "require-dev": {
        "phpunit/phpunit": "7.0.*"
    },
    "autoload": {
        "psr-4": {
            "Rubix\\Graph\\": "src/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
