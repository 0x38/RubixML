{
    "name": "rubix/ml",
    "type": "library",
    "description": "Rubix ML is a machine learning library that lets you build programs that learn from data in PHP.",
    "homepage": "https://github.com/RubixML/RubixML",
    "license": "MIT",
    "keywords": [
        "machine", "learning", "data", "science", "mining", "predictive", "modeling", "classification", "regression", "clustering", "anomaly", "detection", "neural", "network"
    ],
    "authors": [
        {
            "name": "Andrew DalPino",
            "email": "me@andrewdalpino.com"
        }
    ],
    "require": {
        "php": ">=7.1.3",
        "intervention/image": "^2.4",
        "markrogoyski/math-php": "^0.43.0"
    },
    "require-dev": {
        "phpstan/phpstan": "^0.10.0",
        "phpstan/phpstan-phpunit": "^0.10.0",
        "phpunit/phpunit": "7.2.*"
    },
    "suggest": {
        "ext-gd": "For image vectorization"
    },
    "autoload": {
        "psr-4": {
            "Rubix\\ML\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Rubix\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "analyze": "phpstan analyse -c phpstan.neon",
        "test": "phpunit"
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
