{
    "name": "rubix/engine",
    "description": "A library for building intelligent systems with PHP.",
    "license": "MIT",
    "keywords": ["php", "data", "science", "machine", "learning", "mining", "predictive", "modeling"],
    "authors": [
        {
            "name": "Andrew DalPino",
            "email": "me@andrewdalpino.com"
        }
    ],
    "require": {
        "php": ">=7.1.3",
        "league/csv": "^9.1",
        "markrogoyski/math-php": "^0.40.0"
    },
    "require-dev": {
        "phpunit/phpunit": "7.0.*"
    },
    "autoload": {
        "psr-4": {
            "Rubix\\Engine\\": "src/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
