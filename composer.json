{
    "name": "rubix/ml",
    "description": "Rubix is a library that lets you build intelligent programs that learn from data in PHP.",
    "license": "MIT",
    "keywords": ["data", "science", "mining", "machine", "learning", "predictive", "modeling", "ai", "classification", "regression", "clustering"],
    "authors": [
        {
            "name": "Andrew DalPino",
            "email": "me@andrewdalpino.com"
        }
    ],
    "require": {
        "php": ">=7.1.3",
        "intervention/image": "^2.4",
        "league/csv": "^9.1",
        "markrogoyski/math-php": "^0.43.0"
    },
    "require-dev": {
        "phpunit/phpunit": "7.0.*"
    },
    "autoload": {
        "psr-4": {
            "Rubix\\ML\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Rubix\\Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
